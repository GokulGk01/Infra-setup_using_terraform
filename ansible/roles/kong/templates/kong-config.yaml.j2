_format_version: "1.1"
plugins:
- config:
    credentials: false
    exposed_headers:
    - '*'
    - Authorization
    - Content-Type
    headers:
    - '*'
    - X-LANGUAGE-CODE
    - Authorization
    - X-REQUEST-TYPE
    - X-BEARER-TOKEN
    - X-REQUEST-ORIGIN
    - X-Forwarded-For
    - Content-Type
    max_age: 10000
    methods:
    - GET
    - PUT
    - DELETE
    - OPTIONS
    - POST
    origins:
    - https://{{ frontend_url_domain }}
    - https://{{ api_url_domain }}
    - https://www.gstatic.com
    - http://localhost:4200
    - http://localhost:4300
    - https://{{ studio_url_domain }}/
    preflight_continue: false
  enabled: true
  name: cors
  protocols:
  - grpc
  - grpcs
  - http
  - https
services:
- connect_timeout: 60000
  host: admin
  name: admin-api-service
  port: 80
  protocol: http
  read_timeout: 60000
  retries: 5
  routes:
  - https_redirect_status_code: 426
    methods:
    - GET
    - PUT
    - POST
    - DELETE
    - OPTIONS
    name: admin-api-route
    path_handling: v1
    paths:
    - /admin
    preserve_host: false
    protocols:
    - http
    - https
    regex_priority: 0
    strip_path: true
  write_timeout: 60000
